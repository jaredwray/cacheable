{
	"name": "@cacheable/memory",
	"version": "1.0.0",
	"description": "High Performance In-Memory Cache for Node.js",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jaredwray/cacheable.git",
		"directory": "packages/cacheable"
	},
	"author": "Jared Wray <me@jaredwray.com>",
	"license": "MIT",
	"private": false,
	"scripts": {
		"build": "rimraf ./dist && tsup src/index.ts --format cjs,esm --dts --clean",
		"prepublish": "pnpm build",
		"test": "biome check --write && vitest run --coverage",
		"test:ci": "biome check && vitest run --coverage",
		"clean": "rimraf ./dist ./coverage ./node_modules"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.0",
		"@faker-js/faker": "^9.9.0",
		"@keyv/redis": "^5.0.0",
		"@keyv/valkey": "^1.0.7",
		"@types/node": "^24.1.0",
		"@vitest/coverage-v8": "^3.2.4",
		"lru-cache": "^11.1.0",
		"rimraf": "^6.0.1",
		"tsup": "^8.5.0",
		"typescript": "^5.8.3",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@cacheable/memoize": "workspace:^",
		"@cacheable/utils": "workspace:^",
		"@keyv/bigmap": "^1.0.0",
		"hookified": "^1.10.0",
		"keyv": "^5.4.0"
	},
	"keywords": [
		"cacheable",
		"high performance",
		"layer 1 caching",
		"layer 2 caching",
		"distributed caching",
		"Keyv storage engine",
		"memory caching",
		"LRU cache",
		"expiration",
		"CacheableMemory",
		"offline support",
		"distributed sync",
		"secondary store",
		"primary store",
		"non-blocking operations",
		"cache statistics",
		"layered caching",
		"fault tolerant",
		"scalable cache",
		"in-memory cache",
		"distributed cache",
		"lruSize",
		"lru",
		"multi-tier cache"
	],
	"files": [
		"dist",
		"LICENSE"
	]
}
