{
	"name": "@cacheable/benchmark",
	"version": "1.0.0",
	"description": "Cacheable Benchmark Library",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jaredwray/cacheable.git",
		"directory": "packages/node-cache"
	},
	"author": "Jared Wray <me@jaredwray.com>",
	"license": "MIT",
	"private": false,
	"keywords": [
		"cache",
		"caching",
		"benchmark",
		"node",
		"nodejs",
		"cacheable",
		"cacheable-benchmark"
	],
	"scripts": {
		"build": "echo 'no build needed'",
		"prepublish": "pnpm build",
		"lint": "biome check --write --error-on-warnings",
		"test": "pnpm lint && echo 'no tests needed'",
		"test:ci": "biome check --error-on-warnings && echo 'no tests needed'",
		"benchmark:memory": "tsx ./memory.ts && tsx ./memory-lru.ts",
		"benchmark:cache": "tsx ./cache-get-set.ts",
		"benchmark:nodecache": "tsx ./node-cache.ts",
		"clean": "rimraf ./dist ./coverage ./node_modules"
	},
	"dependencies": {
		"@cacheable/memory": "workspace:^",
		"@faker-js/faker": "^9.8.0",
		"@monstermann/tinybench-pretty-printer": "^0.1.0",
		"bentocache": "^1.4.0",
		"cache-manager": "workspace:^",
		"cacheable": "workspace:^",
		"lru.min": "^1.1.2",
		"quick-lru": "^7.0.1",
		"tinybench": "^4.0.1"
	},
	"files": [
		"dist",
		"license"
	],
	"devDependencies": {
		"@biomejs/biome": "^2.2.0",
		"@cacheable/node-cache": "^1.5.6",
		"node-cache": "^5.1.2",
		"tsx": "^4.19.4"
	}
}
